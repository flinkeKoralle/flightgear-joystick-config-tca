<?xml version="1.0"?>

<PropertyList>
    <button n="3">
        <desc type="string">A/P Disconnect</desc>
        <binding>
            <command type="string">property-assign</command>
            <property>controls/autoflight/disconnect</property>
            <value type="bool">true</value>
        </binding>
        <mod-up>
            <binding>
                <command type="string">property-assign</command>
                <property>controls/autoflight/disconnect</property>
                <value type="bool">false</value>
            </binding>
        </mod-up>
    </button>
</PropertyList>
